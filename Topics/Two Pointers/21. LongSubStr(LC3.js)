// O(n)
var lengthOfLongestSubstring = function(s) {
  const set = new Set();
   let [l, r, max] = [0, 0, 0];
    while (r < s.length){
        while (set.has(s[r])) {
            set.delete(s[l]);
            l++;
        }
        set.add(s[r]);
        max = Math.max(max, set.size);
        r++;
    }
    return max;
    
    
};
